{
  "name": "@prompt-enhancer/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "prompt-enhance": "./dist/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.mjs",
    "start": "NODE_OPTIONS=\"--loader ts-node/esm\" node --experimental-specifier-resolution=node -r ts-node/register src/index.tsx",
    "dev": "ts-node --esm src/index.tsx enhance",
    "link": "npm link"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@prompt-enhancer/core": "0.1.0",
    "@huggingface/inference": "^2.6.4",
    "commander": "^11.1.0",
    "ink": "^5.0.0",
    "react": "^18.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@jest/globals": "^29.7.0",
    "ink-testing-library": "^3.0.0"
  }
}
